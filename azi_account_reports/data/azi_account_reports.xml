        <field name="groupby">account_id</field>
        <field name="formulas">balance = AZBS_CL.balance + AZBS_NCL.balance</field>
        <field name="sequence" eval="2"/>
        <field name="green_on_positive" eval="False"/>
        <field name="level" eval="1" />
    </record>
        <record id="account_financial_report_total_current_liabilities1" model="account.financial.html.report.line">
            <field name="name">Current Liabilities</field>
            <field name="code">AZBS_CL</field>
            <field name="parent_id" ref='account_financial_report_liabilities_view1'/>
            <field name="formulas">balance = AZBS_ACL.balance + AZBS_ACP.balance + AZBS_STP.balance + AZBS_TP.balance + AZBS_OCL.balance + AZBS_PAY.balance</field>
            <field name="sequence" eval="1"/>
            <field name="green_on_positive" eval="False"/>
            <field name="level" eval="2" />
        </record>

