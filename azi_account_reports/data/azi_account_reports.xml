<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

<template id="azi_account_reports_backend" inherit_id="web.assets_backend" active="True">
    <xpath expr="." position="inside">
        <link rel="stylesheet" href="/azi_account_reports/static/src/less/azi_account_financial_report.less" />
    </xpath>
</template>

<!-- This template is only used when printing a financial report to a PDF output -->
<template id="azi_assets_financial_report" inherit_id="account_reports.assets_financial_report">
    <xpath expr="." position="inside">
        <link rel="stylesheet" href="/azi_account_reports/static/src/less/azi_account_financial_report.less"/>
    </xpath>
</template>

<!-- Inventory Balance -->
<record id="account_financial_report_invbalance" model="account.financial.html.report">
    <field name="name">Inventory Balance</field>
    <field name="debit_credit" eval="False"/>
    <field name="report_type">no_date_range_analytic</field>
    <field name='parent_id' ref='account_reports.account_reports_business_statements_menu'/>
</record>

<record id="account_financial_report_total_inventory" model="account.financial.html.report.line">
    <field name="name">INVENTORY</field>
    <field name="code">AZIB_INV</field>
    <field name="financial_report_id" ref="account_financial_report_invbalance"/>
    <field name="formulas">balance = AZIB_FG.balance + AZIB_RM.balance + AZIB_O.balance</field>
    <field name="sequence" eval="1"/>
    <field name="level" eval="0" />
</record>
    <record id="account_financial_report_fg_group" model="account.financial.html.report.line">
        <field name="name">Finished Goods Inventory</field>
        <field name="code">AZIB_FG</field>
        <field name="parent_id" ref='account_financial_report_total_inventory'/>
        <field name="domain" eval="[('account_id.tag_ids', 'in', ref('azi_account_reports.azi_account_tag_fg_inventory'))]" />
        <field name="groupby">product_categ_id</field>
        <field name="formulas">balance = sum.balance</field>
        <field name="sequence" eval="1"/>
        <field name="level" eval="2" />
    </record>
    <record id="account_financial_report_rm_group" model="account.financial.html.report.line">
        <field name="name">Raw Materials Inventory</field>
        <field name="code">AZIB_RM</field>
        <field name="parent_id" ref='account_financial_report_total_inventory'/>
        <field name="domain" eval="[('account_id.tag_ids', 'in', ref('azi_account_reports.azi_account_tag_rm_inventory'))]" />
        <field name="groupby">product_categ_id</field>
        <field name="formulas">balance = sum.balance</field>
        <field name="sequence" eval="2"/>
        <field name="level" eval="2" />
    </record>
    <record id="account_financial_report_other_group" model="account.financial.html.report.line">
        <field name="name">Other Inventory</field>
        <field name="code">AZIB_O</field>
        <field name="parent_id" ref='account_financial_report_total_inventory'/>
        <field name="domain" eval="[('account_id.tag_ids', 'in', ref('azi_account_reports.azi_account_tag_other_inventory'))]" />
        <field name="groupby">account_id</field>
        <field name="formulas">balance = sum.balance</field>
        <field name="sequence" eval="3"/>
        <field name="level" eval="2" />
    </record>


<!-- PROFIT AND LOSS
*************************************************
    Net Income
        + Earnings Before Taxes
            + Earnings Before Interest and Taxes
                + Net Operating Income
                    + Gross Profit
                        + Operating Revenue
                        - Cost of Revenue
                            + Materials
                            + Overhead
                            + Salary and Wages
                            + Other
                    - Sales/Marketing Expense
                        + Fixed
                        + Salary and wages
                        + Variable
                        + Travel
                        + Occupancy
                        + Marketing
                        + Other
                    - General/Admin Expense
                        + Fixed
                        + Salary and wages
                        + Variable
                        + Travel
                        + Occupancy
                        + Other
                - Depreciation & Amortization Expense
            + Interest Income
            - Interest Expense
            + Gain or Loss on Sale of Assets
            + Other Income or Expense
        - Income Taxes
*************************************************-->

<record id="account_financial_report_azi_profitandloss1" model="account.financial.html.report">
    <field name="name">AZI Profit and Loss</field>
    <field name="debit_credit" eval="False"/>
    <field name="report_type">date_range_analytic</field>
    <field name='parent_id' ref='account_reports.account_reports_legal_statements_menu'/>
</record>

<record id="account_financial_report_net_profit1" model="account.financial.html.report.line">
    <field name="name">NET PROFIT</field>
    <field name="code">AZPL_NEP</field>
    <field name="financial_report_id" ref="account_financial_report_azi_profitandloss1"/>
    <field name="formulas">balance = AZPL_EBT.balance - AZPL_TAX.balance</field>
    <field name="sequence" eval="1"/>
    <field name="level" eval="0" />
</record>

    <record id="account_financial_report_earnings_before_taxes1" model="account.financial.html.report.line">
        <field name="name">Earnings Before Taxes</field>
        <field name="code">AZPL_EBT</field>
        <field name="formulas">balance = AZPL_EBIT.balance + AZPL_INTI.balance - AZPL_INTE.balance + AZPL_GOA.balance + AZPL_OIE.balance</field>
        <field name="parent_id" ref='account_financial_report_net_profit1'/>
        <field name="sequence" eval="1"/>
        <field name="level" eval="1" />
    </record>

        <record id="account_financial_report_earnings_before_interest_taxes1" model="account.financial.html.report.line">
            <field name="name">Earnings Before Interest and Taxes</field>
            <field name="code">AZPL_EBIT</field>
            <field name="formulas">balance = AZPL_NOI.balance - AZPL_DEP.balance</field>
            <field name="parent_id" ref='account_financial_report_earnings_before_taxes1'/>
            <field name="sequence" eval="1"/>
            <field name="level" eval="2" />
        </record>

            <record id="account_financial_report_net_operating_income1" model="account.financial.html.report.line">
                <field name="name">Net Operating Income</field>
                <field name="code">AZPL_NOI</field>
                <field name="parent_id" ref='account_financial_report_earnings_before_interest_taxes1'/>
                <field name="formulas">balance = AZPL_GRP.balance - AZPL_EXPSM.balance - AZPL_EXPGA.balance</field>
                <field name="sequence" eval="1"/>
                <field name="level" eval="3" />
            </record>

                <record id="account_financial_report_gross_profit1" model="account.financial.html.report.line">
                    <field name="name">Gross Profit</field>
                    <field name="code">AZPL_GRP</field>
                    <field name="formulas">balance = AZPL_OPINC.balance - AZPL_COS.balance</field>
                    <field name="parent_id" ref='account_financial_report_net_operating_income1'/>
                    <field name="sequence" eval="1"/>
                    <field name="level" eval="4" />
                </record>

                    <record id="account_financial_report_revenue1" model="account.financial.html.report.line">
                        <field name="name">Operating Income</field>
                        <field name="code">AZPL_OPINC</field>
                        <field name="formulas">balance = -sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_gross_profit1'/>
                        <field name="domain" eval="[('account_id.user_type_id', 'in', [ref('account.data_account_type_revenue'), ref('account.data_account_type_other_income')])]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="1"/>
                        <field name="level" eval="5" />
                    </record>

                    <record id="account_financial_report_cost_sales1" model="account.financial.html.report.line">
                        <field name="name">Cost of Revenue</field>
                        <field name="code">AZPL_COS</field>
                        <field name="formulas">balance = AZPL_COGM.balance + AZPL_COGO.balance + AZPL_COGW.balance + AZPL_COGF.balance + AZPL_COGS.balance</field>
                        <field name="parent_id" ref='account_financial_report_gross_profit1'/>
                        <field name="sequence" eval="2"/>
                        <field name="level" eval="5" />
                    </record>
                        <record id="account_financial_report_cost_sales_material1" model="account.financial.html.report.line">
                            <field name="name">COGS Material</field>
                            <field name="code">AZPL_COGM</field>
                            <field name="formulas">balance = sum.balance</field>
                            <field name="parent_id" ref='account_financial_report_cost_sales1'/>
                            <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_cogs_material'))]" />
                            <field name="groupby">account_id</field>
                            <field name="sequence" eval="1"/>
                            <field name="green_on_positive" eval="False"/>
                            <field name="level" eval="6" />
                        </record>
                        <record id="account_financial_report_cost_sales_overhead1" model="account.financial.html.report.line">
                            <field name="name">COGS Overhead</field>
                            <field name="code">AZPL_COGO</field>
                            <field name="formulas">balance = sum.balance</field>
                            <field name="parent_id" ref='account_financial_report_cost_sales1'/>
                            <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_cogs_overhead'))]" />
                            <field name="groupby">account_id</field>
                            <field name="sequence" eval="2"/>
                            <field name="green_on_positive" eval="False"/>
                            <field name="level" eval="6" />
                        </record>
                        <record id="account_financial_report_cost_sales_wages1" model="account.financial.html.report.line">
                            <field name="name">COGS Salary &amp; Wages</field>
                            <field name="code">AZPL_COGW</field>
                            <field name="formulas">balance = sum.balance</field>
                            <field name="parent_id" ref='account_financial_report_cost_sales1'/>
                            <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_cogs_salary_wages'))]" />
                            <field name="groupby">account_id</field>
                            <field name="sequence" eval="3"/>
                            <field name="green_on_positive" eval="False"/>
                            <field name="level" eval="6" />
                        </record>
                        <record id="account_financial_report_cost_sales_freight1" model="account.financial.html.report.line">
                            <field name="name">COGS Freight</field>
                            <field name="code">AZPL_COGF</field>
                            <field name="formulas">balance = sum.balance</field>
                            <field name="parent_id" ref='account_financial_report_cost_sales1'/>
                            <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_cogs_freight'))]" />
                            <field name="groupby">account_id</field>
                            <field name="sequence" eval="4"/>
                            <field name="green_on_positive" eval="False"/>
                            <field name="level" eval="6" />
                        </record>
                        <record id="account_financial_report_cost_sales_other1" model="account.financial.html.report.line">
                            <field name="name">COGS Other</field>
                            <field name="code">AZPL_COGS</field>
                            <field name="formulas">balance = sum.balance</field>
                            <field name="parent_id" ref='account_financial_report_cost_sales1'/>
                            <field name="domain" eval="[('account_id.user_type_id', '=', ref('account.data_account_type_direct_costs'))]" />
                            <field name="groupby">account_id</field>
                            <field name="sequence" eval="5"/>
                            <field name="green_on_positive" eval="False"/>
                            <field name="level" eval="6" />
                        </record>

                <record id="account_financial_report_expenses_sm1" model="account.financial.html.report.line">
                    <field name="name">Sales and Marketing Expenses</field>
                    <field name="code">AZPL_EXPSM</field>
                    <field name="formulas">balance = AZPL_SFEXP.balance + AZPL_SVEXP.balance + AZPL_SWEXP.balance + AZPL_SOEXP.balance + AZPL_STEXP.balance + AZPL_SMEXP.balance</field>
                    <field name="parent_id" ref='account_financial_report_net_operating_income1'/>
                    <field name="sequence" eval="2"/>
                    <field name="green_on_positive" eval="False"/>
                    <field name="level" eval="4" />
                </record>
                    <record id="account_financial_report_sm_fixed1" model="account.financial.html.report.line">
                        <field name="name">Fixed Expenses</field>
                        <field name="code">AZPL_SFEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_sm1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_sales_fixed'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="1"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_sm_variable1" model="account.financial.html.report.line">
                        <field name="name">Variable Expenses</field>
                        <field name="code">AZPL_SVEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_sm1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_sales_variable'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="2"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_exp_sm_wages1" model="account.financial.html.report.line">
                        <field name="name">Salary &amp; Wages Expenses</field>
                        <field name="code">AZPL_SWEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_sm1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_sales_salary_wages'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="3"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_exp_sm_occupancy1" model="account.financial.html.report.line">
                        <field name="name">Occupancy Expenses</field>
                        <field name="code">AZPL_SOEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_sm1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_sales_occupancy'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="4"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_exp_sm_travel1" model="account.financial.html.report.line">
                        <field name="name">Travel Expenses</field>
                        <field name="code">AZPL_STEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_sm1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_sales_travel'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="5"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_sm_marketing1" model="account.financial.html.report.line">
                        <field name="name">Marketing Expenses</field>
                        <field name="code">AZPL_SMEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_sm1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_sales_marketing'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="6"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>

                <record id="account_financial_report_expenses_ga1" model="account.financial.html.report.line">
                    <field name="name">General and Administrative Expenses</field>
                    <field name="code">AZPL_EXPGA</field>
                    <field name="formulas">balance = AZPL_GFEXP.balance + AZPL_GVEXP.balance + AZPL_GWEXP.balance + AZPL_GOEXP.balance + AZPL_GTEXP.balance + AZPL_GEXP.balance</field>
                    <field name="parent_id" ref='account_financial_report_net_operating_income1'/>
                    <field name="sequence" eval="3"/>
                    <field name="green_on_positive" eval="False"/>
                    <field name="level" eval="4" />
                </record>
                    <record id="account_financial_report_exp_ga_fixed1" model="account.financial.html.report.line">
                        <field name="name">Fixed Expenses</field>
                        <field name="code">AZPL_GFEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_ga1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_admin_fixed'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="1"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_exp_ga_variable1" model="account.financial.html.report.line">
                        <field name="name">Variable Expenses</field>
                        <field name="code">AZPL_GVEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_ga1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_admin_variable'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="2"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_exp_ga_wages1" model="account.financial.html.report.line">
                        <field name="name">Salary &amp; Wages Expenses</field>
                        <field name="code">AZPL_GWEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_ga1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_admin_salary_wages'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="3"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_exp_ga_occupancy1" model="account.financial.html.report.line">
                        <field name="name">Occupancy Expenses</field>
                        <field name="code">AZPL_GOEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_ga1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_admin_occupancy'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="4"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_exp_ga_travel1" model="account.financial.html.report.line">
                        <field name="name">Travel Expenses</field>
                        <field name="code">AZPL_GTEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_ga1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_admin_travel'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="5"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>
                    <record id="account_financial_report_exp_ga_other1" model="account.financial.html.report.line">
                        <field name="name">Other Expenses</field>
                        <field name="code">AZPL_GEXP</field>
                        <field name="formulas">balance = sum.balance</field>
                        <field name="parent_id" ref='account_financial_report_expenses_ga1'/>
                        <field name="domain" eval="[('account_id.user_type_id', '=', ref('account.data_account_type_expenses'))]" />
                        <field name="groupby">account_id</field>
                        <field name="sequence" eval="6"/>
                        <field name="green_on_positive" eval="False"/>
                        <field name="level" eval="5" />
                    </record>

            <record id="account_financial_report_depreciation1" model="account.financial.html.report.line">
                <field name="name">Depreciation and Amortization Expense</field>
                <field name="code">AZPL_DEP</field>
                <field name="formulas">balance = sum.balance</field>
                <field name="parent_id" ref='account_financial_report_earnings_before_interest_taxes1'/>
                <field name="domain" eval="[('account_id.user_type_id', '=', ref('account.data_account_type_depreciation'))]" />
                <field name="groupby">account_id</field>
                <field name="sequence" eval="2"/>
                <field name="green_on_positive" eval="False"/>
                <field name="level" eval="3" />
            </record>

        <record id="account_financial_report_interest_income1" model="account.financial.html.report.line">
            <field name="name">Interest Income</field>
            <field name="code">AZPL_INTI</field>
            <field name="parent_id" ref='account_financial_report_earnings_before_taxes1'/>
            <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_income_interest'))]" />
            <field name="groupby">account_id</field>
            <field name="formulas">balance = sum.balance</field>
            <field name="sequence" eval="2"/>
            <field name="level" eval="2" />
        </record>

        <record id="account_financial_report_interest_expense1" model="account.financial.html.report.line">
            <field name="name">Interest Expense</field>
            <field name="code">AZPL_INTE</field>
            <field name="parent_id" ref='account_financial_report_earnings_before_taxes1'/>
            <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_expense_interest'))]" />
            <field name="groupby">account_id</field>
            <field name="formulas">balance = sum.balance</field>
            <field name="sequence" eval="3"/>
            <field name="level" eval="2" />
        </record>

        <record id="account_financial_report_gain_loss_sale_assets1" model="account.financial.html.report.line">
            <field name="name">Gain or Loss on Sale of Assets</field>
            <field name="code">AZPL_GOA</field>
            <field name="formulas">balance = -sum.balance</field>
            <field name="parent_id" ref='account_financial_report_earnings_before_taxes1'/>
            <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_gain_loss_sale_assets'))]" />
            <field name="groupby">account_id</field>
            <field name="sequence" eval="4"/>
            <field name="level" eval="2" />
        </record>

        <record id="account_financial_report_other_income_expense1" model="account.financial.html.report.line">
            <field name="name">Other Income or Expense</field>
            <field name="code">AZPL_OIE</field>
            <field name="formulas">balance = -sum.balance</field>
            <field name="parent_id" ref='account_financial_report_earnings_before_taxes1'/>
            <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_other_income_expense'))]" />
            <field name="groupby">account_id</field>
            <field name="sequence" eval="5"/>
            <field name="level" eval="2" />
        </record>

    <record id="account_financial_report_taxes1" model="account.financial.html.report.line">
        <field name="name">Income Taxes</field>
        <field name="code">AZPL_TAX</field>
        <field name="formulas">balance = sum.balance</field>
        <field name="parent_id" ref='account_financial_report_net_profit1'/>
        <field name="domain" eval="[('account_id.user_type_id', '=', ref('azi_account_reports.account_type_taxes'))]" />
        <field name="groupby">account_id</field>
        <field name="sequence" eval="2"/>
        <field name="level" eval="1" />
    </record>


<!-- BALANCE SHEET
*************************************************
Net Assets
    + Assets
        + Current Assets
            + Bank and Cash Accounts
            + Receivables
            + Prepayments
            + Current Assets
                + Inventory
                + Inside Advances
                + Outside Advances
                + Other Current Assets
        + Fixed Assets
        + Long Term and Goodwill
    - Liabilities
        + Current Liabilities
            + Accrued Liabilities
            + Accrued Payroll
            + Sales Tax Payable
            + Taxes Payable
            + Other Current Liabilities
            + Payable
        + Non-current Liabilities
Equity
    + Current Year Earnings
    + Retained Earnings
*************************************************-->

