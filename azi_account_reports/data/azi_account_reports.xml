<record id="account_financial_report_equity1" model="account.financial.html.report.line">
    <field name="name">EQUITY</field>
    <field name="code">AZBS_EQ</field>
    <field name="formulas">balance = AZBS_UNAFFECTED_EARNINGS.balance + AZBS_RETAINED_EARNINGS.balance</field>
    <field name="sequence" eval="2"/>
    <field name="financial_report_id" ref="account_financial_report_balancesheet1"/>
    <field name="level" eval="0" />
</record>
    <record id="account_financial_current_year_earnings1" model="account.financial.html.report.line">
        <field name="name">CURRENT YEAR EARNINGS</field>
        <field name="code">AZBS_UNAFFECTED_EARNINGS</field>
        <field name="domain" eval="[('account_id.user_type_id', '=', ref('account.data_unaffected_earnings'))]" />
        <field name="formulas">balance = AZPL_NEP.balance - sum.balance</field>
        <field name="sequence" eval="1"/>
        <field name="special_date_changer">from_beginning</field>
        <field name="parent_id" ref='account_financial_report_equity1'/>
        <field name="level" eval="1" />
    </record>
    <record id="account_financial_retained_earnings1" model="account.financial.html.report.line">
        <field name="name">RETAINED EARNINGS</field>
