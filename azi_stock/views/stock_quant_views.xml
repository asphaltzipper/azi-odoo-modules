<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="print_fg_report_action" model="ir.actions.server">
        <field name="name">FG Stock Valuation Report</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="azi_stock.model_stock_quant"/>
        <field name="binding_model_id" ref="azi_stock.model_stock_quant" />
        <field name="state">code</field>
        <field name="code">action = records.action_print_report()</field>
    </record>

    <record id="view_stock_quant_tree_inventory_editable" model="ir.ui.view" >
        <field name="name">stock.quant.inventory.tree.editable</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"/>
        <field name="arch" type="xml">
            <field name="inventory_diff_quantity" position="after">
                <field name="inventory_value"/>
            </field>
        </field>
    </record>
</odoo>